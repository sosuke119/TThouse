
<div class="col-md-12 text-center" style="padding: 40px;">
	<%= form_tag import_properties_path , multipart: true, method: :post, class: "form-inline", role: "form" do %>
		<div class="form-group font-thin-bg">
			<%= file_field_tag :file %>
		</div>
		<div class="form-group">
			<%= submit_tag "上傳", class:"btn btn-sm btn-green" %>
		</div>
	<% end %>
</div>